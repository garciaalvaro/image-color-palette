{
	"name": "image-color-palette",
	"version": "1.4.2",
	"description": "Image Color Palette WordPress Plugin",
	"homepage": "https://wordpress.org/plugins/image-color-palette/",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"dev": "webpack --mode development --watch --config ./webpack/build.config.babel.js",
		"prod": "webpack --mode production --config ./webpack/build.config.babel.js",
		"copy": "webpack --mode production --config ./webpack/copy.config.babel.js && rm _release/_temp.js",
		"version": "webpack --mode production --config ./webpack/version.config.babel.js && rm _temp.js",
		"release": "rm -rf build _release && npm run type && npm run version && npm run prod && npm run copy",
		"type": "tsc",
		"lint": "eslint 'src/**/*.{ts,tsx}'",
		"prettier": "prettier --check 'src/**/*.{ts,tsx}'"
	},
	"dependencies": {
		"copy-text-to-clipboard": "^2.1.1",
		"react-tiny-popover": "^4.0.0",
		"rgbquant": "^1.1.2",
		"tinycolor2": "^1.4.1"
	},
	"devDependencies": {
		"@babel/core": "^7.8.7",
		"@babel/preset-env": "^7.8.7",
		"@babel/preset-react": "^7.8.3",
		"@babel/preset-typescript": "^7.8.3",
		"@babel/register": "^7.8.6",
		"@types/lodash": "^4.14.149",
		"@types/node": "^13.9.1",
		"@types/tinycolor2": "^1.4.2",
		"@types/wordpress__block-editor": "^2.2.6",
		"@types/wordpress__blocks": "^6.4.5",
		"@types/wordpress__components": "^8.5.2",
		"@types/wordpress__compose": "^3.4.0",
		"@types/wordpress__data": "^4.6.6",
		"@types/wordpress__edit-post": "^3.5.1",
		"@types/wordpress__element": "^2.4.0",
		"@types/wordpress__hooks": "^2.4.0",
		"@types/wordpress__i18n": "^3.4.0",
		"@types/wordpress__plugins": "^2.3.4",
		"@typescript-eslint/eslint-plugin": "^2.23.0",
		"@typescript-eslint/parser": "^2.23.0",
		"babel-loader": "^8.0.6",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^5.1.1",
		"css-loader": "^3.4.2",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-react": "^7.19.0",
		"file-loader": "^5.1.0",
		"mini-css-extract-plugin": "^0.9.0",
		"nib": "^1.1.2",
		"optimize-css-assets-webpack-plugin": "^5.0.3",
		"prettier": "^1.19.1",
		"string-replace-loader": "^2.2.0",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2",
		"typescript": "^3.8.3",
		"webpack": "^4.42.0",
		"webpack-cli": "^3.3.11"
	}
