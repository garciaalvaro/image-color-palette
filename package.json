{
	"name": "image-color-palette",
	"version": "2.0.0",
	"description": "Image Color Palette Gutenberg Plugin",
	"homepage": "https://wordpress.org/plugins/image-color-palette/",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"prebuild": "npm run test:types && npm run clean",
		"build": "webpack --mode=production",
		"predeploy": "npm run test && npm run clean",
		"deploy": "webpack --mode=production",
		"postdeploy": "node scripts/replace-version.js && node scripts/copy-files.js",
		"clean": "npm run clean:dist && npm run clean:release",
		"clean:dist": "rm -rf dist",
		"clean:release": "rm -rf _release",
		"dev": "webpack --mode=development",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier",
		"test:types": "tsc",
		"test:lint": "eslint \"**/*.{js,ts,tsx}\"",
		"test:prettier": "prettier --check \"**/*.{js,ts,tsx,php,json}\"",
		"prettier": "prettier --write \"**/*.{js,ts,tsx,php,json}\""
	},
	"dependencies": {
		"@babel/core": "^7.14.0",
		"@babel/preset-env": "^7.14.1",
		"@babel/preset-react": "^7.13.13",
		"@babel/preset-typescript": "^7.13.0",
		"@babel/register": "^7.13.16",
		"@types/lodash": "^4.14.168",
		"@types/tinycolor2": "^1.4.2",
		"@types/wordpress__block-editor": "^2.2.9",
		"@types/wordpress__components": "^9.8.6",
		"@types/wordpress__compose": "^3.4.3",
		"@types/wordpress__data": "^4.6.9",
		"@types/wordpress__edit-post": "^3.5.4",
		"@types/wordpress__plugins": "^2.3.7",
		"@wordpress/browserslist-config": "^3.0.3",
		"@wordpress/hooks": "^2.12.3",
		"@wordpress/i18n": "^3.20.0",
		"babel-loader": "^8.2.2",
		"browserslist": "^4.16.6",
		"copy-text-to-clipboard": "^3.0.1",
		"copyfiles": "^2.4.1",
		"css-loader": "^5.2.4",
		"css-minimizer-webpack-plugin": "^2.0.0",
		"mini-css-extract-plugin": "^1.6.0",
		"nib": "^1.1.2",
		"react-tiny-popover": "^6.0.5",
		"replace-in-file": "^6.2.0",
		"rgbquant": "^1.1.2",
		"stylus": "^0.54.8",
		"stylus-loader": "^5.0.0",
		"terser-webpack-plugin": "^5.1.1",
		"tinycolor2": "^1.4.2",
		"typescript": "^4.2.4",
		"webpack": "^5.76.0",
		"webpack-cli": "^4.7.0"
	},
	"devDependencies": {
		"@prettier/plugin-php": "^0.16.2",
		"@typescript-eslint/eslint-plugin": "^4.22.1",
		"@typescript-eslint/parser": "^4.22.1",
		"eslint": "^7.26.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.23.2",
		"prettier": "^2.2.1"
	}
}
