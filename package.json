{
	"name": "image-color-palette",
	"version": "1.5.0",
	"description": "Image Color Palette Gutenberg Plugin",
	"homepage": "https://wordpress.org/plugins/image-color-palette/",
	"author": "Alvaro Garcia",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"prebuild": "npm run test:types && npm run clean:dist && npm run clean:release",
		"build": "webpack --mode=production",
		"postbuild": "node scripts/replace-version.js && node scripts/copy-files.js",
		"clean:dist": "rm -rf dist",
		"clean:release": "rm -rf _release",
		"dev": "webpack --mode=development",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier",
		"test:types": "tsc",
		"test:lint": "eslint \"**/*.{js,ts,tsx}\"",
		"test:prettier": "prettier --check \"**/*.{js,ts,tsx,php,json}\"",
		"prettier": "prettier --write \"**/*.{js,ts,tsx,php,json}\""
	},
	"dependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-react": "^7.12.10",
		"@babel/preset-typescript": "^7.12.7",
		"@babel/register": "^7.12.10",
		"@types/lodash": "^4.14.165",
		"@types/node": "^14.14.14",
		"@types/tinycolor2": "^1.4.2",
		"@wordpress/block-editor": "^5.2.0",
		"@wordpress/blocks": "^6.25.0",
		"@wordpress/browserslist-config": "^2.7.0",
		"@wordpress/components": "^12.0.0",
		"@wordpress/compose": "^3.23.0",
		"@wordpress/data": "^4.26.0",
		"@wordpress/edit-post": "^3.26.0",
		"@wordpress/hooks": "^2.11.0",
		"@wordpress/i18n": "^3.17.0",
		"@wordpress/plugins": "^2.24.0",
		"babel-loader": "^8.2.2",
		"browserslist": "^4.16.0",
		"copy-text-to-clipboard": "^2.2.0",
		"copyfiles": "^2.4.1",
		"css-loader": "^5.0.1",
		"css-minimizer-webpack-plugin": "^1.1.5",
		"mini-css-extract-plugin": "^1.3.3",
		"nib": "^1.1.2",
		"react": "^16.14.0",
		"react-tiny-popover": "^6.0.3",
		"replace-in-file": "^6.1.0",
		"rgbquant": "^1.1.2",
		"stylus": "^0.54.8",
		"stylus-loader": "^4.3.1",
		"terser-webpack-plugin": "^5.0.3",
		"tinycolor2": "^1.4.2",
		"typescript": "^4.1.3",
		"webpack": "^5.11.0",
		"webpack-cli": "^4.2.0"
	},
	"devDependencies": {
		"@prettier/plugin-php": "^0.16.1",
		"@typescript-eslint/eslint-plugin": "^4.10.0",
		"@typescript-eslint/parser": "^4.10.0",
		"eslint": "^7.16.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-prettier": "^3.3.0",
		"eslint-plugin-react": "^7.21.5",
		"prettier": "^2.2.1"
	}
}
