msgstr "Complementarios"

#: image-color-palette.php:86
msgid "Lighter"
msgstr "Claros"

#: image-color-palette.php:87
msgid "Brighter"
msgstr "Brillantes"

#: image-color-palette.php:88
msgid "Darker"
msgstr "Oscuros"

#: image-color-palette.php:89
msgid "Desaturated"
msgstr "Desaturados"

#: image-color-palette.php:90
msgid "Saturated"
msgstr "Saturados"

